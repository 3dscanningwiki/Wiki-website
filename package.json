{
  "name": "3dscanning.wiki",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "npm run copy-images && svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "copy": "rm -rf content/* && cp -r ../content/* content/ && rm -rf content/.git",
    "copy-images": "(cd content; cp --parents `find . -iname '*.png' -o -iname '*.jp*g' -iname '*.gif' -o -iname '*.webp'` ../static/)"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.26",
    "@sveltejs/kit": "1.0.0-next.250",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "svelte": "^3.44.0"
  },
  "type": "module",
  "dependencies": {
    "fs-extra": "^10.0.0",
    "fuse.js": "^6.5.3",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.3.2",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-front-matter": "^0.2.3",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-replace-link": "^1.1.0"
  }
}
